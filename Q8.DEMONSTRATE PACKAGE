// INSIDE THE PACKAGE
package pac1;
 public class emp
 {
	 public void show()
	 {
		 System.out.println("class emp show");
	 }
 }
	 
	 class dept
	 {
		 void disp()
		 {
			 emp e = new emp();
			 e.show();
			 System.out.println("class depyth disp");
		 }
	 }
			 /// OUTSIDE THE PACKAGE

import pac1.emp.*;
